{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "andes_app": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "options": {
                        "outputPath": "dist",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "tsConfig": "tsconfig.json",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "src/assets",
                                "output": "/assets"
                            },
                            {
                                "glob": "favicon.ico",
                                "input": "src",
                                "output": "/"
                            },
                            {
                                "glob": "**/*",
                                "input": "src/app/modules/rup/assets/",
                                "output": "/assets"
                            }
                        ],
                        "styles": [
                            "src/styles.scss",
                            "./node_modules/ol/ol.css",
                            "./node_modules/vis-timeline/styles/vis-timeline-graph2d.min.css"
                        ],
                        "scripts": [
                            "node_modules/intro.js/intro.js"
                        ]
                    },
                    "configurations": {
                        "dev": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.dev.ts"
                                }
                            ],
                            "buildOptimizer": false,
                            "sourceMap": true,
                            "optimization": false,
                            "aot": true
                        },
                        "test": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.test.ts"
                                }
                            ],
                            "buildOptimizer": false,
                            "sourceMap": true,
                            "optimization": false,
                            "aot": true
                        },
                        "demo": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.demo.ts"
                                }
                            ]
                        },
                        "production": {
                            "optimization": false,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-builders/custom-webpack:dev-server",
                    "options": {
                        "browserTarget": "andes_app:build:dev"
                    },
                    "configurations": {
                        "test": {
                            "browserTarget": "andes_app:build:test"
                        },
                        "dev": {
                            "browserTarget": "andes_app:build:dev"
                        },
                        "production": {
                            "browserTarget": "andes_app:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "andes_app:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "include": [
                            "src/**/*.spec.ts"
                        ],
                        "karmaConfig": "./karma.conf.js",
                        "scripts": [
                            "./node_modules/intro.js/intro.js"
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "src/assets",
                                "output": "/assets"
                            },
                            {
                                "glob": "favicon.ico",
                                "input": "src",
                                "output": "/"
                            }
                        ]
                    }
                }
            }
        },
        "auth": {
            "projectType": "library",
            "root": "projects/auth",
            "sourceRoot": "projects/auth/src",
            "prefix": "auth",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/auth/tsconfig.lib.json",
                        "project": "projects/auth/ng-package.json"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/auth/src/test.ts",
                        "tsConfig": "projects/auth/tsconfig.spec.json",
                        "karmaConfig": "projects/auth/karma.conf.js"
                    }
                }
            }
        },
        "shared": {
            "projectType": "library",
            "root": "projects/shared",
            "sourceRoot": "projects/shared/src",
            "prefix": "shared",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/shared/tsconfig.lib.json",
                        "project": "projects/shared/ng-package.json"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/shared/src/test.ts",
                        "tsConfig": "projects/shared/tsconfig.spec.json",
                        "karmaConfig": "projects/shared/karma.conf.js"
                    }
                }
            }
        },
        "portal": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/portal",
            "sourceRoot": "projects/portal/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/portal",
                        "index": "projects/portal/src/index.html",
                        "main": "projects/portal/src/main.ts",
                        "polyfills": "projects/portal/src/polyfills.ts",
                        "tsConfig": "projects/portal/tsconfig.app.json",
                        "assets": [
                            "projects/portal/src/favicon.ico",
                            "projects/portal/src/assets"
                        ],
                        "styles": [
                            "projects/portal/src/styles.scss"
                        ],
                        "scripts": [],
                        "vendorChunk": true,
                        "extractLicenses": false,
                        "aot": true,
                        "buildOptimizer": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true
                    },
                    "configurations": {
                        "dev": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/portal/src/environments/environment.ts",
                                    "with": "projects/portal/src/environments/environment.dev.ts"
                                }
                            ],
                            "buildOptimizer": false,
                            "sourceMap": true
                        },
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/portal/src/environments/environment.ts",
                                    "with": "projects/portal/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "aot": true
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "portal:build:dev"
                    },
                    "configurations": {
                        "dev": {
                            "browserTarget": "portal:build:dev"
                        },
                        "production": {
                            "browserTarget": "portal:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "portal:build"
                    }
                }
            }
        },
        "form-builder": {
            "projectType": "library",
            "root": "projects/form-builder",
            "sourceRoot": "projects/form-builder/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/form-builder/tsconfig.lib.json",
                        "project": "projects/form-builder/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/form-builder/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/form-builder/src/test.ts",
                        "tsConfig": "projects/form-builder/tsconfig.spec.json",
                        "karmaConfig": "projects/form-builder/karma.conf.js"
                    }
                }
            }
        }
    },
    "schematics": {
        "@schematics/angular:component": {
            "prefix": "app",
            "style": "css"
        },
        "@schematics/angular:directive": {
            "prefix": "app"
        }
    },
    "cli": {
        "analytics": false
    }
}